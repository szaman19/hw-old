import java.io.*;
import java.util.*;

public class Sort {
    
    public int[] merge(int[] A, int[] B){
	int[] C = new int[A.length + B.length];
	int a = 0;
	int b = 0;
	for (int i = 0 ; i < C.length; i++){
	    if (A[a] < B[b]){
		C[i] = A[a];
		a++;
	    }
	    else{
		C[i] = B[b];
		b++;
	    }
	}
	return C;
    }

    
}