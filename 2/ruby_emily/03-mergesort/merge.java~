import java.io.*;
import java.util.*;

public class merge{
    public int[] merge(int[] x, int[] y){
	int xcount = 0;
	int ycount = 0;
	int[] result = new int[x.length + y.length];
	for (int i=0;i<x.length+y.length;i++){
	    if (x[xcount] < y[ycount]){
		result[i] = y[ycount];
		ycount++;
	    }
	}
	return result;
    }
}
