public class Person implements Comparable{
    private int age;
    private String name;
    
    public int getAge(){
	return age;
    }

    public String getName(){
	return name;
    }

    public String toString(){
	return "Name: " + name + ", age: " + age;
    }
    
    public int compareTo(Person o){
	//if (this.getAge() > o.getAge()){
	if (this.age > o.age){
	    return 1;
	}
	if (this.age < o.age){
	    return -1;
	}
	return 0;

    }
}
