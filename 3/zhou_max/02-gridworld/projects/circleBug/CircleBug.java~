import info.gridworld.actor.Bug; 
 

public class CircleBug extends Bug 
{ 
    // private int steps; 
    // private int sideLength; 
 

    public BoxBug(int length) 
    { 
	
    } 
 
 
    /** 
     * Moves to the next location of the square. 
     */ 
    public void act() 
    { 
	move();
	if (steps < sideLength && canMove()) 
	    { 
		move(); 
	    } 
    } 
}
